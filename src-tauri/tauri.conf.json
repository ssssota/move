{
	"build": {
		"beforeDevCommand": "pnpm dev",
		"beforeBuildCommand": "pnpm build",
		"devPath": "http://localhost:1420",
		"distDir": "../dist"
	},
	"package": {
		"productName": "move",
		"version": "0.0.1"
	},
	"tauri": {
		"allowlist": {
			"all": false,
			"shell": {
				"all": false,
				"open": true
			}
		},
		"windows": [
			{
				"title": "move",
				"width": 400,
				"height": 200
			}
		],
		"security": {
			"csp": null
		},
		"bundle": {
			"active": true,
			"targets": "all",
			"identifier": "dev.ssssota.move",
			"icon": [
				"icons/32x32.png",
				"icons/128x128.png",
				"icons/128x128@2x.png",
				"icons/icon.icns",
				"icons/icon.ico"
			]
		}
	}
}
